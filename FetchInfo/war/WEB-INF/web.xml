<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/fetchinfo/greet</url-pattern>
  </servlet-mapping>
 
 
   <servlet>
    <servlet-name>CreateDeals</servlet-name>
    <servlet-class>com.CreateDeals</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>CreateDeals</servlet-name>
    <url-pattern>/CreateDeals</url-pattern>
  </servlet-mapping>
 
 
   <servlet>
    <servlet-name>GetInfo</servlet-name>
    <servlet-class>com.GetInfo</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>GetInfo</servlet-name>
    <url-pattern>/GetInfo</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>GetDeals</servlet-name>
    <servlet-class>com.GetDeals</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>GetDeals</servlet-name>
    <url-pattern>/GetDeals</url-pattern>
  </servlet-mapping> 
  
   <servlet>
    <servlet-name>CreateDatabase</servlet-name>
    <servlet-class>com.CreateDatabase</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>CreateDatabase</servlet-name>
    <url-pattern>/CreateDatabase</url-pattern>
  </servlet-mapping> 
  
   <servlet>
    <servlet-name>MyJSon</servlet-name>
    <servlet-class>com.MyJSon</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>MyJSon</servlet-name>
    <url-pattern>/json</url-pattern>
  </servlet-mapping> 
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
